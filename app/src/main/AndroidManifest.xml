<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="hi.parkpirates.android">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".controller.SeekActivity">
        </activity>
        <activity android:name=".controller.BuryActivity">
        </activity>
        <activity android:name=".controller.TreasureInfoActivity">
        </activity>
        <activity android:name=".controller.UserInfoActivity">
        </activity>
        <activity android:name=".controller.MapActivity">
        </activity>
        <activity android:name=".controller.RegisterActivity">
        </activity>
        <activity android:name=".controller.LoginActivity"
            android:label="PpDebug">
            <intent-filter>
                <action android:name=".controller.LoginActivity"/>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".StartGame">
        </activity>
    </application>

</manifest>